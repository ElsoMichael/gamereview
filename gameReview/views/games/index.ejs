<div class="container moreSpace moreSpaceBottom">
  <div class="row">
    <% list.forEach((game) => { %>
      <% if(game.cover){%>
        <div class="col-sm-12 game-search-spacing">
          <a href="/games/<%= game.id %>"><img src="<%= game.cover.url %>"></a>
          <h4 class="game-name"><a class="a-link" href="/games/<%= game.id %>"><%= game.name %></a></h4>
        </div>
      <% } %>
    <% }); %>
  </div>
</div>